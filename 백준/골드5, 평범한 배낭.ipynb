    "# 14:39 ~ 15:05 X\n",
    "\n",
    "N, K = map(int, input().split())\n",
    "dp = [0] * (K+1)\n",
    "stuff = []\n",
    "for _ in range(N):\n",
    "    W, V = map(int, input().split())\n",
    "    stuff.append((W, V))\n",
    "\n",
    "for w, v in stuff:\n",
    "    for i in range(K, w-1, -1):\n",
    "        dp[i] = max(dp[i], dp[i-w]+v)\n",
    "\n",
    "print(dp[K])"
   ]
  }
