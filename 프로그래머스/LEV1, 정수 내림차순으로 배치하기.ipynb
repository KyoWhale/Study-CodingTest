    "# 14:09 ~ 14:10\n",
    "\n",
    "def solution(n):\n",
    "    counts = [0] * 10\n",
    "    for c in str(n):\n",
    "        counts[int(c)] += 1\n",
    "\n",
    "    answer = \"\"\n",
    "    for i in range(9, -1, -1):\n",
    "        answer += str(i) * counts[i]\n",
    "    return int(answer)\n",
