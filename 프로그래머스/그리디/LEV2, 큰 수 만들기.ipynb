    "# 17:32 ~ X 답 봄\n",
    "\n",
    "def solution(number, k):\n",
    "    answer = []\n",
    "    \n",
    "    for n in number:\n",
    "        while answer and answer[-1] < n and k > 0:\n",
    "            answer.pop()\n",
    "            k -= 1\n",
    "\n",
    "        answer.append(n)\n",
    "\n",
    "    return \"\".join(answer[:len(answer)-k])\n",
    "\n",
    "solution(\"4177252841\", 4)"
   ]
  }
