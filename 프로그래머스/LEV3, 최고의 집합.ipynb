    "# 16:28 ~ X\n",
    "\n",
    "def solution(n, s):\n",
    "    answer = []\n",
    "    \n",
    "    share = s // n\n",
    "    remain = s % n\n",
    "\n",
    "    if share == 0:\n",
    "        return [-1]\n",
    "\n",
    "    answer += [share] * (n - remain)\n",
    "    answer += [share+1] * remain\n",
    "\n",
    "    return answer\n",
    "\n",
    "solution(2, 9)"
   ]
  }
